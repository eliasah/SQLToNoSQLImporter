<?xml version="1.0" encoding="UTF-8"?>

<dataConfig>
	<!-- PostgreSQL config -->
	<!-- <dataSource driver="org.postgresql.Driver" url="jdbc:postgresql://localhost/testing" 
		user="postgres" password="testing" batch-size="-1" /> -->

	<!-- MySQL config -->
	<dataSource type="JdbcDataSource" driver="com.mysql.jdbc.Driver"
		url="jdbc:mysql://127.0.0.1/dev" user="root" password="root"
		batch-size="-1" />

	<document>
		<entity name="PRODUCTS" query="select * from PRODUCTS">
			<field column="id" name="id" type="integer" />
			<field column="title" name="title" type="string" />
			<field column="ean" name="ean" type="string"/>
			<entity name="PRODUCTS" multiValued="true" query="SELECT * FROM PRODUCTS">
				<field name="dev" column="id" type="integer" />
			</entity>
		</entity>
	</document>
</dataConfig>
